pipeline {
          agent any
          tools {
                  maven "maven"
                }
                stages{
                        stage('check-out') {
                        steps {
                                echo 'git'
                                git branch: 'main', url: 'https://github.com/edureka27/webapp.git'
                               }
                                            }
                        stage('compile') {
                        steps {
                                echo 'compiling'
                                mvn compile
                               }
                                          }
                        stage('test') {
                        steps {
                               echo 'testing'
                                mvn test
                               }
                                       }
                        stage('deploy') {
                        steps {
                               echo 'deploying'
                              mvn install
                               }
                                        }
                      }
}
      
