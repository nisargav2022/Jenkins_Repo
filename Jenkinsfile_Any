pipeline {
          agent any
environment {
         PATH = "$PATH:/usr/share/maven/bin"
    }
                stages{
                        stage('check-out') {
                        steps {
                                echo 'git'
                                git branch: 'main', url: 'https://github.com/akshu20791/DevOpsClassCodes.git'
                               }
                                            }
                        stage('compile') {
                        steps {
                                echo 'compiling'
                                echo "Hi"
                               }
                                          }
                        stage('test') {
                        steps {
                               echo 'testing'
                                mvn test
                               }
                                       }
                        stage('deploy') {
                        steps {
                               echo 'deploying'
                              mvn install
                               }
                                        }
                      }
}
      
